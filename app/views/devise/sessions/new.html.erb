<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
  <div class="field">
    <%= f.label :user_id, "ログインID*" %><%= f.email_field :UserID, autofocus: true, autocomplete: "UserID" %>

    <%= f.label :UserName, "ユーザー名:" %><%= f.email_field :UserName, autofocus: true, autocomplete: "UserName" %>さんようこそ
  </div>
   
  <div class="field">
    <%= f.label :password, "パスワード*" %>
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= f.submit "ログイン" %>
 
<% end %>

    <button onclick="window.close()">閉じる</button>
  </div>

<%= link_to 'メニューへ', menu_path %>